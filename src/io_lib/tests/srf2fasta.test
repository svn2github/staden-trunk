#!/bin/sh
slx_out=$top_srcdir/tests/data/slx_out

$top_builddir/progs/srf2fasta proc.srf > slx.fasta
cmp slx.fasta $slx_out/slx.fasta || exit 1
$top_builddir/progs/srf2fasta -C proc.srf > slx.fasta
cmp slx.fasta $slx_out/slx-C.fasta || exit 1

$top_builddir/progs/srf2fasta raw.srf > slx.fasta
cmp slx.fasta $slx_out/slx.fasta || exit 1
$top_builddir/progs/srf2fasta -C raw.srf > slx.fasta
cmp slx.fasta $slx_out/slx-C.fasta || exit 1

$top_builddir/progs/srf2fasta both.srf > slx.fasta
cmp slx.fasta $slx_out/slx.fasta || exit 1
$top_builddir/progs/srf2fasta -C both.srf > slx.fasta
cmp slx.fasta $slx_out/slx-C.fasta || exit 1
